@model IEnumerable<LocadoraWeb.Model.Cliente>

@{
    ViewData["Title"] = "Segundo cliente que mais alugou filmes";
    int contador = 1;
}

<h1>Segundo cliente que mais alugou filmes</h1>
<table class="table">
    <thead>
        <tr>
            <th>nº</th>
            <th>Nome</th>
            <th>CPF</th>
            <th>Data de nascimento</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@contador</td>
                <td>@item.Nome</td>
                <td>@item.CPF</td>
                <td>@item.DataNascimento.ToString("dd/MM/yyyy")</td>
            </tr>
            contador++;
        }
    </tbody>
</table>
